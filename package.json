{
  "name": "moleculer-realworld-example-app",
  "version": "1.0.0",
  "description": "RealWorld example app with Moleculer microservices framework",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services",
    "start": "moleculer-runner",
    "deps": "npm-check -u",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services",
    "docker:build": "docker build -t conduit ."
  },
  "keywords": [
    "microservices",
    "moleculer",
    "realworld"
  ],
  "author": "",
  "devDependencies": {
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "moleculer-repl": "^0.6.2",
    "npm-check": "^5.9.0"
  },
  "dependencies": {
    "bcrypt": "3.0.7",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moleculer": "^0.14.0-beta6",
    "moleculer-db": "^0.8.4",
    "moleculer-db-adapter-mongo": "^0.4.6",
    "moleculer-web": "^0.9.0-beta7",
    "nats": "^1.3.2",
    "slug": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moleculerjs/moleculer-realworld-example-app.git"
  },
  "engines": {
    "node": ">= 10.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}